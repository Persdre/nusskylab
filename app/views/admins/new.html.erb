<% content_for :main_content do %>
  <% javascript 'admins.js' %>
  <div class="panel panel-default">
    <div class="panel-heading">
      <h2 class="text-center">
        Create an admin in cohort <%= cohort %>
      </h2>
    </div>
    <div class="panel-body">
      <%= render 'admin_form', locals: {
        admin: @role, users: users, cohort: cohort, is_new: true
      } %>
    </div>
  </div>
<% end %>
