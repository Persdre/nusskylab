<ul class="nav nav-pills nav-stacked col-md-2" id="current_cohorts">
  <li class="active"><a data-toggle="pill" href="#vostok">Vostok</a></li>
  <li><a data-toggle="pill" href="#project_gemini">Project Gemini</a></li>
  <li><a data-toggle="pill" href="#apollo_11">Apollo 11</a></li>
</ul>
<div class="tab-content col-md-10">
  <% @teams_table.each do |project_level, teams| %>
    <div id="<%= project_level %>" class=<%= get_project_class_attribute(project_level)%> >
      <% if teams.length > 0 %>
        <%= render 'submissions_display', locals: {teams: teams,
                                                   selected_type: project_level} %>
      <% else %>
        <p>This level currently has no submissions</p>
      <% end %>
    </div>
  <% end %>
</div>

